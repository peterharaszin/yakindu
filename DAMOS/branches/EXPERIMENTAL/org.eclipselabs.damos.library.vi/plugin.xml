<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.emf.ecore.uri_mapping">
      <mapping
            source="http://www.eclipselabs.org/damos/library/vi/1.0.0/"
            target="platform:/plugin/org.eclipselabs.damos.library.vi/blocktypes/">
      </mapping>
   </extension>
   <extension
         point="org.eclipselabs.damos.dml.blockGroups">
      <group
            id="org.eclipselabs.damos.library.vi.communication"
            name="Communication"
            supergroup="org.eclipselabs.damos.library.vi">
      </group>
      <group
            id="org.eclipselabs.damos.library.vi.controls"
            name="Controls"
            supergroup="org.eclipselabs.damos.library.vi">
      </group>
      <group
            id="org.eclipselabs.damos.library.vi.indicators"
            name="Indicators"
            supergroup="org.eclipselabs.damos.library.vi">
      </group>
      <group
            id="org.eclipselabs.damos.library.vi"
            name="Damos Virtual Instruments">
      </group>
   </extension>
   <extension
         point="org.eclipselabs.damos.dml.blockTypes">
      <blockType
            group="org.eclipselabs.damos.library.vi.communication"
            qualifiedName="org.eclipselabs.damos.library.vi.communication.UDPSource"
            name="UDPSource"
            uri="http://www.eclipselabs.org/damos/library/vi/1.0.0/communication/UDPSource.blocktype.dml#/">
      </blockType>
      <blockType
            group="org.eclipselabs.damos.library.vi.communication"
            qualifiedName="org.eclipselabs.damos.library.vi.communication.UDPSink"
            name="UDPSink"
            uri="http://www.eclipselabs.org/damos/library/vi/1.0.0/communication/UDPSink.blocktype.dml#/">
      </blockType>
      <blockType
            group="org.eclipselabs.damos.library.vi.controls"
            qualifiedName="org.eclipselabs.damos.library.vi.controls.Slider"
            name="Slider"
            uri="http://www.eclipselabs.org/damos/library/vi/1.0.0/controls/Slider.blocktype.dml#/">
      </blockType>
      <blockType
            group="org.eclipselabs.damos.library.vi.indicators"
            qualifiedName="org.eclipselabs.damos.library.vi.indicators.Gauge"
            name="Gauge"
            uri="http://www.eclipselabs.org/damos/library/vi/1.0.0/indicators/Gauge.blocktype.dml#/">
      </blockType>
   </extension>
   <extension
         point="org.eclipselabs.damos.execution.core.blockSignaturePolicies">
      <policy
            blockType="org.eclipselabs.damos.library.vi.communication.UDPSource"
            class="org.eclipselabs.damos.library.vi.signatures.communication.UDPSourceSignaturePolicy"
            name="UDPSource">
      </policy>
      <policy
            blockType="org.eclipselabs.damos.library.vi.communication.UDPSink"
            class="org.eclipselabs.damos.library.vi.signatures.communication.UDPSinkSignaturePolicy"
            name="UDPSink">
      </policy>
      <policy
            blockType="org.eclipselabs.damos.library.vi.controls.Slider"
            class="org.eclipselabs.damos.library.vi.signatures.controls.SliderSignaturePolicy"
            name="Slider">
      </policy>
      <policy
            blockType="org.eclipselabs.damos.library.vi.indicators.Gauge"
            class="org.eclipselabs.damos.library.vi.signatures.indicators.GaugeSignaturePolicy"
            name="Gauge">
      </policy>
   </extension>

</plugin>
