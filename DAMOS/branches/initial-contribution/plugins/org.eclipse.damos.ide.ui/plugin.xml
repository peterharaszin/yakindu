<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="quickFixProviders" name="Quick Fix Providers" schema="schema/quickFixProviders.exsd"/>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.eclipse.damos.ide.ui.IDEUIExecutableExtensionFactory:org.eclipse.damos.diagram.ui.preferences.BlockDiagramPreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.eclipse.damos.ide.ui.IDEUIExecutableExtensionFactory:org.eclipse.damos.diagram.ui.parts.BlockDiagramEditor"
            contributorClass="org.eclipse.damos.ide.ui.IDEUIExecutableExtensionFactory:org.eclipse.damos.diagram.ui.parts.BlockDiagramActionBarContributor"
            default="true"
            extensions="blockdiagram"
            icon="icons/BlockDiagram.png"
            id="org.eclipse.damos.ide.ui.blockDiagramEditor"
            matchingStrategy="org.eclipse.gmf.runtime.diagram.ui.resources.editor.parts.DiagramDocumentEditorMatchingStrategy"
            name="Block Diagram Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.eclipse.damos.ide.ui.damos"
            name="Damos">
      </category>
      <view
            category="org.eclipse.damos.ide.ui.damos"
            class="org.eclipse.damos.ide.ui.IDEUIExecutableExtensionFactory:org.eclipse.damos.dml.ui.parts.FragmentExplorerView"
            icon="icons/FragmentExplorer.png"
            id="org.eclipse.damos.ide.ui.fragmentExplorerView"
            name="Fragment Explorer"
            restorable="true">
      </view>
      <view
            category="org.eclipse.damos.ide.ui.damos"
            class="org.eclipse.damos.ide.ui.IDEUIExecutableExtensionFactory:org.eclipse.damos.dml.ui.parts.BlockLibraryView"
            icon="icons/BlockLibrary.png"
            id="org.eclipse.damos.ide.ui.blockLibraryView"
            name="Block Library">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.damos.ide.ui.damosCategory"
            name="Damos">
      </category>
      <wizard
            category="org.eclipse.damos.ide.ui.damosCategory"
            class="org.eclipse.damos.ide.ui.wizards.NewDamosProjectWizard"
            finalPerspective="org.eclipse.damos.ide.ui.damosPerspective"
            icon="icons/DamosProject.gif"
            id="org.eclipse.damos.ide.ui.newDamosProjectWizard"
            name="Damos Project"
            project="true">
      </wizard>
      <wizard
            category="org.eclipse.damos.ide.ui.damosCategory"
            class="org.eclipse.damos.ide.ui.IDEUIExecutableExtensionFactory:org.eclipse.damos.diagram.ui.wizards.NewBlockDiagramCreationWizard"
            icon="icons/BlockDiagram.png"
            id="org.eclipse.damos.ide.ui.blockDiagramWizard"
            name="Block Diagram">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.eclipse.damos.ide.ui.fragmentExplorerView">
         <command
               commandId="org.eclipse.damos.diagram.ui.commands.newFragment"
               icon="icons/newfile_wiz.gif"
               label="New Fragment"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.damos.ide.ui.fragmentExplorerView?after=file">
         <command
               commandId="org.eclipse.damos.diagram.ui.commands.newFragment"
               icon="icons/newfile_wiz.gif"
               label="New..."
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.damos.ide.ui.fragmentExplorerView?after=edit">
         <command
               commandId="org.eclipse.damos.diagram.ui.commands.deleteFragment"
               icon="icons/delete.gif"
               label="Delete"
               style="push">
         </command>
         <command
               commandId="org.eclipse.damos.diagram.ui.commands.renameFragment"
               label="Rename..."
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.eclipse.damos.ide.ui.internal.DamosPerspectiveFactory"
            icon="icons/BlockDiagram.png"
            id="org.eclipse.damos.ide.ui.damosPerspective"
            name="Damos">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="org.eclipse.damos.ide.ui.properties">
         <propertyCategory
               category="default">
         </propertyCategory>
         <propertyCategory
               category="extended">
         </propertyCategory>
         <propertyCategory
               category="advanced">
         </propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.eclipse.damos.ide.ui.properties">
         <propertyTab
               category="default"
               id="org.eclipse.damos.ide.ui.tabs.general"
               label="General">
         </propertyTab>
         <propertyTab
               afterTab="org.eclipse.damos.ide.ui.tabs.general"
               category="default"
               id="org.eclipse.damos.ide.ui.tabs.parameters"
               label="Parameters">
         </propertyTab>
         <propertyTab
               afterTab="org.eclipse.damos.ide.ui.tabs.parameters"
               category="extended"
               id="org.eclipse.damos.ide.ui.tabs.appearance"
               label="Appearance">
         </propertyTab>
         <propertyTab
               afterTab="org.eclipse.damos.ide.ui.tabs.appearance"
               category="extended"
               id="org.eclipse.damos.ide.ui.tabs.diagram"
               label="Rulers &amp; Grid">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="org.eclipse.damos.ide.ui.properties">
         <propertySection
               class="org.eclipse.damos.ide.ui.IDEUIExecutableExtensionFactory:org.eclipse.damos.dml.ui.properties.GeneralPropertySection"
               enablesFor="1"
               filter="org.eclipse.damos.dml.ui.properties.GeneralPropertySectionFilter"
               id="org.eclipse.damos.ide.ui.sections.blockName"
               tab="org.eclipse.damos.ide.ui.tabs.general">
         </propertySection>
         <propertySection
               afterSection="org.eclipse.damos.ide.ui.sections.blockName"
               class="org.eclipse.damos.ide.ui.IDEUIExecutableExtensionFactory:org.eclipse.damos.dml.ui.properties.TimingPropertySection"
               enablesFor="1"
               filter="org.eclipse.damos.dml.ui.properties.TimingPropertySectionFilter"
               id="org.eclipse.damos.ide.ui.sections.timingConstraint"
               tab="org.eclipse.damos.ide.ui.tabs.general">
         </propertySection>
         <propertySection
               afterSection="org.eclipse.damos.ide.ui.sections.timingConstraint"
               class="org.eclipse.damos.ide.ui.IDEUIExecutableExtensionFactory:org.eclipse.damos.dml.ui.properties.InoutportDataTypePropertySection"
               enablesFor="1"
               filter="org.eclipse.damos.dml.ui.properties.InoutportDataTypePropertySectionFilter"
               id="org.eclipse.damos.ide.ui.sections.inoutportDataType"
               tab="org.eclipse.damos.ide.ui.tabs.general">
         </propertySection>
         <propertySection
               afterSection="org.eclipse.damos.ide.ui.sections.timingConstraint"
               class="org.eclipse.damos.ide.ui.IDEUIExecutableExtensionFactory:org.eclipse.damos.dml.ui.properties.LatchPropertySection"
               enablesFor="1"
               filter="org.eclipse.damos.dml.ui.properties.LatchPropertySectionFilter"
               id="org.eclipse.damos.ide.ui.sections.latch"
               tab="org.eclipse.damos.ide.ui.tabs.general">
         </propertySection>
         <propertySection
               class="org.eclipse.damos.ide.ui.IDEUIExecutableExtensionFactory:org.eclipse.damos.dml.ui.properties.ParametersPropertySection"
               enablesFor="1"
               filter="org.eclipse.damos.dml.ui.properties.ParametersPropertySectionFilter"
               id="org.eclipse.damos.ide.ui.sections.parameters"
               tab="org.eclipse.damos.ide.ui.tabs.parameters">
         </propertySection>
         <propertySection
               class="org.eclipse.gmf.runtime.diagram.ui.properties.sections.appearance.DiagramColorsAndFontsPropertySection"
               filter="org.eclipse.gmf.runtime.diagram.ui.properties.filters.DiagramEditPartPropertySectionFilter"
               id="org.eclipse.damos.ide.ui.sections.diagramColorsAndFonts"
               tab="org.eclipse.damos.ide.ui.tabs.appearance">
         </propertySection>
         <propertySection
               class="org.eclipse.gmf.runtime.diagram.ui.properties.sections.appearance.ShapeColorsAndFontsPropertySection"
               filter="org.eclipse.gmf.runtime.diagram.ui.properties.filters.ShapeEditPartPropertySectionFilter"
               id="org.eclipse.damos.ide.ui.sections.shapeColorsAndFonts"
               tab="org.eclipse.damos.ide.ui.tabs.appearance">
         </propertySection>
         <propertySection
               class="org.eclipse.gmf.runtime.diagram.ui.properties.sections.appearance.ConnectionAppearancePropertySection"
               filter="org.eclipse.gmf.runtime.diagram.ui.properties.filters.ConnectionEditPartPropertySectionFilter"
               id="org.eclipse.damos.ide.ui.sections.connectionAppearance"
               tab="org.eclipse.damos.ide.ui.tabs.appearance">
         </propertySection>
         <propertySection
               afterSection="org.eclipse.damos.ide.ui.sections.shapeColorsAndFonts"
               class="org.eclipse.damos.diagram.ui.properties.ShowComponentNamePropertySection"
               id="org.eclipse.damos.ide.ui.sections.showName"
               tab="org.eclipse.damos.ide.ui.tabs.appearance">
            <input
                  type="org.eclipse.damos.diagram.ui.editparts.ComponentEditPart">
            </input>
         </propertySection>
         <propertySection
               class="org.eclipse.gmf.runtime.diagram.ui.properties.sections.grid.RulerGridPropertySection"
               filter="org.eclipse.gmf.runtime.diagram.ui.properties.filters.DiagramEditPartPropertySectionFilter"
               id="org.eclipse.damos.ide.ui.sections.rulerGrid"
               tab="org.eclipse.damos.ide.ui.tabs.diagram">
         </propertySection>
      </propertySections>
   </extension>
   <extension
         point="org.eclipse.gmf.runtime.common.ui.services.action.globalActionHandlerProviders">
      <GlobalActionHandlerProvider
            class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramGlobalActionHandlerProvider"
            id="org.eclipse.damos.ide.ui.globalActionHandlerProvider">
         <Priority
               name="Lowest">
         </Priority>
         <ViewId
               id="org.eclipse.damos.ide.ui.blockDiagramEditor">
            <ElementType class="org.eclipse.gmf.runtime.diagram.ui.editparts.IGraphicalEditPart">
               <GlobalActionId actionId="save">
               </GlobalActionId>
            </ElementType>
         </ViewId>
      </GlobalActionHandlerProvider>
      <GlobalActionHandlerProvider
            class="org.eclipse.gmf.runtime.diagram.ui.printing.render.providers.DiagramWithPrintGlobalActionHandlerProvider"
            id="org.eclipse.damos.ide.ui.diagramWithPrintGlobalActionHandlerProvider">
         <Priority
               name="Lowest">
         </Priority>
         <ViewId
               id="org.eclipse.damos.ide.ui.blockDiagramEditor">
            <ElementType
                  class="org.eclipse.gmf.runtime.diagram.ui.editparts.IGraphicalEditPart">
               <GlobalActionId
                     actionId="print">
               </GlobalActionId>
            </ElementType>
         </ViewId>
      </GlobalActionHandlerProvider>
   </extension>
   <extension
         point="org.eclipse.gmf.runtime.diagram.ui.editpolicyProviders">
      <editpolicyProvider
            class="org.eclipse.damos.ide.ui.IDEUIExecutableExtensionFactory:org.eclipse.damos.ide.ui.internal.providers.EditPolicyProvider">
         <Priority
               name="Lowest">
         </Priority>
      </editpolicyProvider>
   </extension>
   <extension
         point="org.eclipse.gmf.runtime.diagram.ui.decoratorProviders">
      <decoratorProvider
            class="org.eclipse.damos.ide.ui.internal.providers.NatureCheckingDecoratorProvider">
         <Priority
               name="Lowest">
         </Priority>
      </decoratorProvider>
   </extension>
   <extension
         point="org.eclipse.gmf.runtime.diagram.ui.paletteProviders">
      <paletteProvider
            class="org.eclipse.damos.ide.ui.IDEUIExecutableExtensionFactory:org.eclipse.damos.diagram.ui.palette.PaletteProvider">
         <editor
               id="org.eclipse.damos.ide.ui.blockDiagramEditor">
         </editor>
         <Priority
               name="Lowest">
         </Priority>
      </paletteProvider>
   </extension>
   <extension
         point="org.eclipse.gmf.runtime.diagram.ui.decoratorProviders">
      <decoratorProvider
            class="org.eclipse.damos.ide.ui.internal.providers.ProblemMarkerDecoratorProvider">
         <Priority
               name="Lowest">
         </Priority>
      </decoratorProvider>
   </extension>
   <extension
         point="org.eclipse.damos.dml.ui.injectorProviders">
      <provider
            class="org.eclipse.damos.ide.ui.internal.providers.UIInjectorProvider"
            language="org.eclipse.damos">
      </provider>
   </extension>
   <extension
         point="org.eclipse.damos.ide.ui.quickFixProviders">
      <provider
            class="org.eclipse.damos.ide.ui.internal.providers.QuickFixProvider">
      </provider>
   </extension>
   <extension
         point="org.eclipse.ui.ide.markerResolution">
      <markerResolutionGenerator
            class="org.eclipse.damos.ide.ui.internal.providers.QuickFixMarkerResolutionGenerator"
            markerType="org.eclipse.damos.common.damosProblem">
      </markerResolutionGenerator>
   </extension>
   <extension
         point="org.eclipse.emf.ecore.extension_parser">
      <parser
            class="org.eclipse.damos.ide.ui.IDEUIExecutableExtensionFactory:org.eclipse.damos.ide.core.resource.BlockDiagramResourceFactory"
            type="blockdiagram">
      </parser>
   </extension>

</plugin>
