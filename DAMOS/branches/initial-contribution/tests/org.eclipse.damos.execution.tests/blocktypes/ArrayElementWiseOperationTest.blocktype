package damos.tests

blockType ArrayElementWiseOperationTest {

	input a
	input b
	output y1
	output y2
	
	behavior {
		check(int, int) -> real[3], real[3, 2]

		static assert a is int :
			error "A must be integer"

		static assert b is int :
			error "B must be integer"

		eq y1 = { 10, a, 30 } + { 1, 2.1, b } - { a, 7, 9 }
		eq y2 = { { 10, a }, 10 * { 3, 4 }, { 50, 60 } } + { { 1, 2.1 }, { b, 3 }, { 2, 4 } } - { { a, 7 }, { 3, 3 }, { 1, 2 } }
	}

}
