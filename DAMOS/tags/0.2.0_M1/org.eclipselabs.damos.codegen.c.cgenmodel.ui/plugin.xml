<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.eclipselabs.damos.codegen.c.cgenmodel.ui.editor.presentation.CGenModelModelWizardID"
            name="%_UI_CGenModelModelWizard_label"
            class="org.eclipselabs.damos.codegen.c.cgenmodel.ui.editor.presentation.CGenModelModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/CGenModelModelFile.gif">
         <description>%_UI_CGenModelModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="org.eclipselabs.damos.codegen.c.cgenmodel.ui.editor.presentation.CGenModelEditorID"
            name="%_UI_CGenModelEditor_label"
            icon="icons/full/obj16/CGenModelModelFile.gif"
            extensions="cgenmodel"
            class="org.eclipselabs.damos.codegen.c.cgenmodel.ui.editor.presentation.CGenModelEditor"
            contributorClass="org.eclipselabs.damos.codegen.c.cgenmodel.ui.editor.presentation.CGenModelActionBarContributor">
      </editor>
   </extension>

</plugin>
