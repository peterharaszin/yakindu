<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.eclipselabs.damos.simulation.simulationmodel.ui.editor.presentation.SimulationModelModelWizardID"
            name="%_UI_SimulationModelModelWizard_label"
            class="org.eclipselabs.damos.simulation.simulationmodel.ui.editor.presentation.SimulationModelModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/SimulationModelModelFile.gif">
         <description>%_UI_SimulationModelModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="org.eclipselabs.damos.simulation.simulationmodel.ui.editor.presentation.SimulationModelEditorID"
            name="%_UI_SimulationModelEditor_label"
            icon="icons/full/obj16/SimulationModelModelFile.gif"
            extensions="simulationmodel"
            class="org.eclipselabs.damos.simulation.simulationmodel.ui.editor.presentation.SimulationModelEditor"
            contributorClass="org.eclipselabs.damos.simulation.simulationmodel.ui.editor.presentation.SimulationModelActionBarContributor">
      </editor>
   </extension>

</plugin>
