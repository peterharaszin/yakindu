<plugin>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.eclipse.damos.ide.ui.properties">
         <propertyTab
               afterTab="org.eclipse.damos.dml.ui.tabs.general"
               category="default"
               id="org.eclipse.damos.library.base.ui.tabs.math.ports"
               label="Ports">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="org.eclipse.damos.ide.ui.properties">
         <propertySection
               afterSection="org.eclipse.damos.diagram.ui.sections.showName"
               class="org.eclipse.damos.library.base.ui.properties.LogicGateShapePropertySection"
               id="org.eclipse.damos.library.base.ui.sections.logic.logicGateShape"
               tab="org.eclipse.damos.ide.ui.tabs.appearance">
            <input
                  type="org.eclipse.damos.library.base.ui.editparts.LogicGateEditPart">
            </input>
         </propertySection>
         <propertySection
               class="org.eclipse.damos.library.base.ui.properties.SumInputPortsPropertySection"
               id="org.eclipse.damos.library.base.ui.sections.math.inputPorts"
               tab="org.eclipse.damos.library.base.ui.tabs.math.ports">
            <input
                  type="org.eclipse.damos.library.base.ui.editparts.SumEditPart">
            </input>
         </propertySection>
         <propertySection
               afterSection="org.eclipse.damos.diagram.ui.sections.showName"
               class="org.eclipse.damos.library.base.ui.properties.SumShapePropertySection"
               id="org.eclipse.damos.library.base.ui.sections.math.sumShape"
               tab="org.eclipse.damos.ide.ui.tabs.appearance">
            <input
                  type="org.eclipse.damos.library.base.ui.editparts.SumEditPart">
            </input>
         </propertySection>
      </propertySections>
   </extension>
   <extension
         point="org.eclipse.damos.diagram.ui.blockViewFactories">
