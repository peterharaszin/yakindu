            blockType="damos.blocks.And"
            class="org.eclipse.damos.library.base.ui.view.factories.LogicGateViewFactory"
            name="And">
      </viewFactory>
      <viewFactory
            blockType="damos.blocks.Or"
            class="org.eclipse.damos.library.base.ui.view.factories.LogicGateViewFactory"
            name="Or">
      </viewFactory>
      <viewFactory
            blockType="damos.blocks.Inverter"
            class="org.eclipse.damos.library.base.ui.view.factories.LogicGateViewFactory"
            name="Inverter">
      </viewFactory>
      <viewFactory
            blockType="damos.blocks.Sum"
            class="org.eclipse.damos.library.base.ui.view.factories.SumViewFactory"
            name="Sum">
         <input
               class="org.eclipse.damos.library.base.ui.view.factories.SumInputPortViewFactory"
               name="input">
         </input>
      </viewFactory>
   </extension>
