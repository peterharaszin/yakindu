<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="componentGeneratorProviders" name="Component Generator Providers" schema="schema/componentGeneratorProviders.exsd"/>
   <extension-point id="blockGenerators" name="Block Generators" schema="schema/blockGenerators.exsd"/>
   <extension-point id="runtimeEnvironmentAPIs" name="Runtime Environment APIs" schema="schema/runtimeEnvironmentAPIs.exsd"/>
   <extension
         point="org.eclipselabs.damos.codegen.c.generator.componentGeneratorProviders">
      <provider
            class="org.eclipselabs.damos.codegen.c.generator.internal.registry.BlockGeneratorProvider">
      </provider>
      <provider
            class="org.eclipselabs.damos.codegen.c.generator.internal.providers.BehavioredBlockGeneratorProvider">
      </provider>
      <provider
            class="org.eclipselabs.damos.codegen.c.generator.internal.providers.BuiltinComponentGeneratorProvider">
      </provider>
   </extension>
   <extension
         point="org.eclipselabs.damos.dconfig.definitions">
      <resource
            uri="platform:/plugin/org.eclipselabs.damos.codegen.c.generator/dconfig/CodegenC.xmi">
      </resource>
   </extension>
   <extension
         point="org.eclipselabs.damos.codegen.generators">
      <generator
            class="org.eclipselabs.damos.codegen.c.generator.Generator"
            id="damos.codegen.c.DefaultGenerator"
            name="Default Generator">
      </generator>
   </extension>

</plugin>
