               class="org.eclipselabs.damos.library.base.ui.properties.logic.LogicGateShapePropertySection"
               id="org.eclipselabs.damos.library.base.ui.sections.logic.logicGateShape"
               tab="org.eclipselabs.damos.ide.ui.tabs.appearance">
            <input
                  type="org.eclipselabs.damos.library.base.ui.editparts.logic.LogicGateEditPart">
            </input>
         </propertySection>
