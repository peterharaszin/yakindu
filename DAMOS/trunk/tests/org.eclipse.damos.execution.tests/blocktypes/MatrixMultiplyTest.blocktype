package damos.tests

blockType MatrixMultiplyTest {

	input a
	input b
	output y
	
	behavior {
		check(int, int) -> real[4, 2]

		static assert a is int :
			error "A must be integer"

		static assert b is int :
			error "B must be integer"

		eq y = { { 1, a, 3 }, { 4, 5.1, 6 }, { 7, 8, 9 }, { 10, 11, 12 } } * { { 13, 14 }, { b, 16 }, { 17, 18 } }
	}

}
