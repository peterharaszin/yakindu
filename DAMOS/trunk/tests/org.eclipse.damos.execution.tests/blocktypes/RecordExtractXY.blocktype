package damos.tests

blockType RecordExtractXY {

	input x
	output outX
	output outY
	
	type MyNamedRecord = {
		int x;
		int y;
	}

	behavior {
		check({ int x; int y }) -> int, int

		static assert x is { int x; int y } :
			error "X must be { int x; int y }"

		eq outX = x.x
		eq outY = x.y
	}

}
