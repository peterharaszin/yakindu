package damos.tests

blockType StructExtractXY {

	input x
	output outX
	output outY
	
	type MyNamedStruct = {
		int x;
		int y;
	}

	function main(x) -> outX, outY {
		check({ int x; int y }) -> int, int

		static assert x is { int x; int y } :
			error "X must be { int x; int y }"

		eq outX = x.x
		eq outY = x.y
	}

}
