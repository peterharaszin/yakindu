package damos.tests

blockType StructExtractXY {

	input x
	output outX
	output outY
	
	struct MyNamedStruct {
		int x;
		int y;
	}

	function main(x) -> outX, outY {
		check(struct { int x; int y }) -> int, int

		static assert x is struct { int x; int y } :
			error "X must be struct { int x; int y }"

		eq outX = x.x
		eq outY = x.y
	}

}
