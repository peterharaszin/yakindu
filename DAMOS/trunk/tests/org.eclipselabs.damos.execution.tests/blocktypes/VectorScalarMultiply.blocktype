package damos.tests

blockType VectorScalarMultiply {

	input x
	output y

	parameter gain1 = 7
	parameter gain2 = 1.7

	behavior {
		check(real[3], <1>, <1>) -> real[3]

		static assert gain1 is real(?) :
			error "Gain must be real value"

		static assert gain2 is real(?) :
			error "Gain must be real value"

		static assert x is real(?)[] :
			error "X must be real[] value"

		eq y = gain1 * x * gain2
	}

}
