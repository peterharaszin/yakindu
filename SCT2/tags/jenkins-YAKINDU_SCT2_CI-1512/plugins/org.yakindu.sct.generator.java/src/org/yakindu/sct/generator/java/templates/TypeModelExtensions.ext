import types;

boolean isVoid(Type this) :
	name == null || name == "void";
	
String getJavaType(Type this) :
		switch (name) {
		case "real" : "double"
		case "integer" : "int"
		case "boolean" : "boolean"
		case "string" : "String"
		case "void" : "void"
		default : "//"+this
		};
		
String getJavaClassType(Type this) :
		switch (name) {
		case "real" : "Double"
		case "integer" : "Integer"
		case "boolean" : "Boolean"
		case "string" : "String"
		case "void" : "Void"
		case null : "Void"
		default : "//"+this
		};
		
String getInitialValue(Type this) :
		switch (name) {
		case "real" : "0D"
		case "integer" : "0"
		case "boolean" : "false"
		case "string" : "\"\""
		case "void" : "null"
		default : ""
		};
	