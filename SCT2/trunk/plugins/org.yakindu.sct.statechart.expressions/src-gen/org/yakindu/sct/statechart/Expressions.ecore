    <eStructuralFeatures xsi:type="ecore:EReference" name="guardExpression" eType="#//Expression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="action" eType="#//ExpressionRule"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ElementDefinition">
    <eStructuralFeatures xsi:type="ecore:EReference" name="variables" upperBound="-1"
