<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="expressions"
    nsURI="http://www.yakindu.org/sct/statechart/Expressions" nsPrefix="expressions">
  <eClassifiers xsi:type="ecore:EClass" name="EntryRule">
    <eStructuralFeatures xsi:type="ecore:EReference" name="entry" eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StateExpression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="entryExpression" upperBound="-1"
        eType="#//EntryExpression" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="exitExpression" upperBound="-1"
        eType="#//ExitExpression" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="ontickExpression" upperBound="-1"
        eType="#//OnTickExpression" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EntryExpression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="expression" eType="#//ExpressionRule"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ExitExpression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="expression" eType="#//ExpressionRule"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OnTickExpression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="expression" eType="#//ExpressionRule"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CustomTransitionExpression">
