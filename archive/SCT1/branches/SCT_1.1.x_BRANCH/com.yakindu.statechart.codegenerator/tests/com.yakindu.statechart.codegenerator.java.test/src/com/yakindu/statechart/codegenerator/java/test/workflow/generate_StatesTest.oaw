<?xml version="1.0"?>
<workflow>
	<!-- src-gen point to output folder for generated code --> 
	<property name="src-gen" value="src-gen" />
	<!-- model is the statemachine model file, not the diagram file -->
	<property name="model" value="model/StatesTest.statemachine"/>
	
	<!-- Generator call with model-file and output-folder -->
    <cartridge file='com/yakindu/statechart/codegenerator/java/workflow/generate_java_defensive.oaw' model="${model}" src-gen="${src-gen}"/>
    
</workflow>